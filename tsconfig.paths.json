{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@assets": ["assets/index.ts"],
      "@assets/*": ["assets/*"],
      "@pages/*": ["pages/*"],
      "@components/*": ["components/*"],
      "@styles/*": ["styles/*"],
      "@recoil/*": ["recoil/*"],
      "@type/*": ["types/*"],
      "@utils/*": ["utils/*"],
      "@constants": ["constants/index.ts"],
      "@constants/*": ["constants/*"],
      "@hooks/*": ["hooks/*"],
      "@router": ["router/index.tsx"]
    }
  }
}
